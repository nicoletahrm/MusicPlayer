@model IEnumerable<DomainModels.MusicType>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<br/>

<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Genre)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr class="table-light">
            <td>
                @Html.DisplayFor(modelItem => item.Genre)
            </td>
            <td>
                @Html.ActionLink("Top songs", "MostLikedPerGenre", new { genre = item.Genre }, new { style="text-decoration: none;" } )
            </td>
        </tr>
}
    </tbody>
</table>